{
  "name": "reacttwilliotextchat",
  "version": "1.0.0",
  "description": "A Text chat using Twillio Channel app in React JS",
  "scripts": {
    "client": "react-scripts start",
    "server": "node server.js",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "keywords": [
    "react",
    "twilio",
    "twilio-chat"
  ],
  "license": "MIT",
  "dependencies": {
    "chance": "^1.0.11",
    "classnames": "^2.2.5",
    "concurrently": "^5.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "prop-types": "^15.5.10",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "^1.0.11",
    "twilio": "^3.6.7",
    "twilio-chat": "^1.1.1"
  },
  "proxy": "http://localhost:3001"
}
